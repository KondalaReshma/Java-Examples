package object_oriented_programming;

public   class SwitchCaseRunner {

	public static void main(String[] args) {
		System.out.println(determineTheWeekDay(2));
	}

	    public static String determineTheWeekDay(int day) {
	    	 
		switch(day) {
		case 1:
			return "sunday";
		case 2:
			return "monday";
		case 3:
			return "tuesday";
		case 4:
			return "wednesday";
		case 5:
			return "thursday";
		case 6:
			return "friday";
		case 7:
			return "saturday";
		default:
			return "default";
		}
	}

}
